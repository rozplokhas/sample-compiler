fun fac(n) begin
 	if n < 2
	then
		return n
	else
		return n * fac(n - 1)
	fi
end

fun pow(x, n) begin
	i := 0;
	res := 1;
	while i < n do
		res := res * x;
		i := i + 1
	od;
	return res
end

fun fib(n) begin
	i  := 0;
	a0 := 0;
	a1 := 1;
	while i < n do
		a1 := a1 + a0;
		a0 := a1 - a0;
		i := i + 1
	od;
	return a0
end

